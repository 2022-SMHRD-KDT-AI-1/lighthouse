  </servlet>
  <servlet-mapping>
  	<servlet-name>ajaxTest</servlet-name>
  	<url-pattern>/ajaxTest</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>reviewService</servlet-name>
  	<url-pattern>/reviewService</url-pattern>
  </servlet-mapping>
